<h1> Edit Product </h1>
<div class="row">
<%= form_for(@product) do |f| %>
	<%= f.label :name %>
	<%= f.text_field :name %>
	<%= f.label :cost, "Price" %>
	<%= f.text_field :cost %>
	<%= f.label :image_url, "Url Image" %>
	<%= f.text_field :image_url  %>
	<%= f.submit "Save Change Product?", class:"btn btn-large btn-primary" %>
	</div>
	<% end %>